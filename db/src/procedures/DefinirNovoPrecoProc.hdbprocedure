PROCEDURE "training.procedures::DefinirNovoPrecoProc.hdbprocedure"(
    IN P_PERCENT DECIMAL
    )
    LANGUAGE SQLSCRIPT
    SQL SECURITY DEFINER
    --DEFAULT SCHEMA "9827C62410384DA98EEDCD3A835EE87E"
AS
BEGIN
    UPDATE TRAINING_PRODUCTS_TBLPRODUCTS
    SET SALESPRICE = SALESPRICE * :P_PERCENT;
END